        <field name="name">commission.target.tree</field>
        <field name="model">commission.target</field>
        <field name="arch" type="xml">
            <tree string="Targets" editable="bottom">
                <field name="period"/>
                <field name="q_start_date" />
                <field name="q_end_date" />
